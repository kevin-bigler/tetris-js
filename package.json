{
  "name": "tetris-js",
  "version": "1.0.0",
  "description": "Tetris for the browser, written in javascript",
  "main": "src/app.js",
  "scripts": {
    "test": "jest src",
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "watch": "parcel watch src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevin-bigler/tetris-js.git"
  },
  "author": "Kevin Bigler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kevin-bigler/tetris-js/issues"
  },
  "homepage": "https://github.com/kevin-bigler/tetris-js#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "key-code": "0.0.1",
    "lodash": "^4.17.4",
    "pixi.js": "^4.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "babel-plugin-lodash": "^3.3.4",
    "jest": "^24.5.0",
    "parcel-bundler": "^1.12.3"
  },
  "jest": {
    "testMatch": ["**/?(*.)+(spec|test).[jt]s?(x)"],
